{
    "name": "agencenous/blocss",
    "version": "0.3.3",
    "description": "CSS editor for elements in Wordpress blocks",
    "type": "wordpress-package",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Bastien Ho",
            "email": "bastien@avecnous.eu"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "files": [
            "blocss.php"
        ]
    },
    "require-dev": {
        "agencenous/bump-version": "dev-main"
    },
    "archive": {
        "exclude": [
            "/node_modules",
            "node_modules/",
            "package.json",
            "package-lock.json",
            ".babelrc",
            ".npmignore",
            "/src"
        ]
    }
}
